<% if (nominations.length) { %>Lunch nominations:
<% nominations.forEach(function(nomination) { %>  + <%= nomination.where %> (<%= nomination.user %>)
<% nomination.votes.forEach(function(user) { %>    - <%= user.name %><% if (user.comment) { %> -- <%= user.comment %><% } %>
<% })}) %><% } else { %>No Lunch nominations<% } %>
